package components

templ SideBar(title string, items []string) {
<aside class="w-64 bg-indigo-700 text-white p-6">
  <h2 class="text-2xl font-bold mb-8">{ title }</h2>
  <nav>
    <ul class="space-y-4">
      for _,item := range items {
      <li><a href="#" class="block hover:bg-indigo-600 p-2 rounded">{ item }</a></li>
      }
    </ul>
  </nav>
</aside>
}
